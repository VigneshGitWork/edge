{
	"name": "skyplus-container-app",
	"version": "1.0.0",
	"description": "Generates an AEM Frontend project with Webpack",
	"repository": {
		"type": "git",
		"url": ""
	},
	"main": "src/main/webpack/site/main.ts",
	"license": "SEE LICENSE IN LICENSE.txt",
	"scripts": {
		"eslint": "npx eslint src/main/webpack/!(core-components)/**/*.js --format table",
		"dev": "webpack --env dev --config ./webpack.dev.js && clientlib --verbose",
		"prod": "webpack --config ./webpack.prod.js && clientlib --verbose",
		"start": "webpack serve --open --config ./webpack.dev.js",
		"sync": "aemsync -d -p ../ui.apps/src/main/content",
		"watch": "webpack serve --config ./webpack.dev.js",
		"storybook": "start-storybook -p 6016 --no-manager-cache -s static",
		"build:storybook": "build-storybook",
		"build:htl": "node ./tools/htl-converter/index.mjs",
		"plop": "plop",
		"start:debug": "node --inspect-brk ./node_modules/.bin/webpack --env dev --config ./webpack.dev.js"
	},
	"lint-staged": {
		"skyplus-container-app/src/main/webpack/!(core-components)/**/*.js": [
			"eslint"
		],
		"*.css": "stylelint",
		"skyplus-container-app/src/main/webpack/**/*.scss": [
			"stylelint"
		],
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@babel/core": "^7.19.1",
		"@babel/plugin-proposal-class-properties": "^7.3.3",
		"@babel/plugin-proposal-decorators": "^7.23.6",
		"@babel/plugin-proposal-export-default-from": "^7.23.3",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.3.2",
		"@babel/plugin-proposal-optional-chaining": "^7.21.0",
		"@babel/plugin-proposal-private-methods": "^7.18.6",
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@babel/preset-env": "^7.19.1",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.23.3",
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"aem-clientlib-generator": "^1.4.3",
		"aemsync": "^4.0.1",
		"autoprefixer": "^9.2.1",
		"babel-loader": "^8.2.5",
		"babel-plugin-macros": "^3.1.0",
		"browserslist": "^4.2.1",
		"chokidar-cli": "^3.0.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^10.1.0",
		"css-loader": "^6.7.1",
		"css-minimizer-webpack-plugin": "^3.2.0",
		"cssnano": "^5.0.12",
		"eslint": "^8.4.1",
		"eslint-formatter-table": "^7.32.1",
		"eslint-webpack-plugin": "^3.1.1",
		"glob-import-loader": "^1.2.0",
		"handlebars-webpack-plugin": "^2.2.1",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.4.5",
		"plop": "^3.1.1",
		"postcss": "^8.2.15",
		"postcss-loader": "^4.0.0",
		"postcss-pixel-to-remvw": "^2.0.6",
		"postcss-prefix-selector": "^1.16.0",
		"postcss-scss": "^4.0.4",
		"prettier": "2.6.2",
		"sass": "^1.51.0",
		"sass-loader": "^12.6.0",
		"source-map-loader": "^0.2.4",
		"style-loader": "^3.3.1",
		"terser-webpack-plugin": "^5.2.5",
		"ts-loader": "^9.2.6",
		"tsconfig-paths-webpack-plugin": "^3.2.0",
		"typescript": "^4.8.3",
		"webpack": "^5.65.0",
		"webpack-bundle-analyzer": "^4.10.1",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.11.1",
		"webpack-merge": "^5.8.0"
	},
	"dependencies": {
		"axios": "^1.1.3",
		"core-js": "^3.35.0",
		"crypto-js": "^4.1.1",
		"external-remotes-plugin": "^1.0.0",
		"html-react-parser": "^3.0.4",
		"jquery": "^3.6.0",
		"lint-staged": "^12.4.1",
		"lodash": "^4.17.21",
		"normalize-scss": "^7.0.1",
		"postcss-scss": "^4.0.4",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"slick-carousel": "^1.8.1",
		"svg-url-loader": "^8.0.0"
	},
	"browserslist": [
		"last 2 version",
		"> 1%"
	]
}
